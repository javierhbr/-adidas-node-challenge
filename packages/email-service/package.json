{
  "name": "email-service",
  "version": "0.0.1",
  "description": "Node challenge Javier Benavides",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=development nodemon --trace-warnings",
    "start:local": "NODE_ENV=local nodemon --trace-warnings",
    "start:debug": "nodemon --config nodemon-debug.json",
    "business:start:dev": "NODE_ENV=development nodemon",
    "business:start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "NODE_ENV=development jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.12",
    "@nestjs/core": "^7.6.12",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/microservices": "^7.6.12",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.12",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^7.1.5",
    "amqp-connection-manager": "^3.2.2",
    "amqplib": "^0.7.1",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "config": "^3.3.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.5",
    "file-type": "^16.2.0",
    "helmet": "^4.4.1",
    "js-yaml": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.28",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nestjs-i18n": "^8.0.11",
    "ngeohash": "^0.6.3",
    "object-hash": "^2.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
